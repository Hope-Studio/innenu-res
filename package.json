{
  "name": "inenu-server",
  "version": "3.0.0-alpha.1",
  "private": true,
  "description": "inNENU 小程序资源文件",
  "scripts": {
    "generate": "node ./script/gen/index.js",
    "lint": "prettier --check --write . && eslint --fix --ext .ts .",
    "lint:check": "prettier --check . && eslint --ext .ts .",
    "ts:compile": "tsc",
    "ts:watch": "tsc --watch",
    "upload": "node ./script/upload.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Hope-Studio/innenu-res.git"
  },
  "license": "Mr.Hope LICENSE",
  "author": {
    "name": "Mr.Hope",
    "email": "zhangbowang1998@gmail.com",
    "url": "https://mrhope.site"
  },
  "dependencies": {
    "@mr-hope/assert-type": "1.0.0",
    "@types/js-yaml": "3.12.5",
    "@types/node": "14.14.7",
    "@typescript-eslint/eslint-plugin": "4.8.1",
    "@typescript-eslint/parser": "4.7.0",
    "axios": "0.21.0",
    "del": "6.0.0",
    "eslint": "7.13.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "ftp-hope": "0.1.1",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "js-yaml": "3.14.0",
    "prettier": "2.2.0",
    "qrcode": "1.4.4",
    "typescript": "4.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run ts:compile && yarn run generate && git add res/config/ && git add resource/ && git add img/"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yaml,md}": "prettier --write"
  }
}
